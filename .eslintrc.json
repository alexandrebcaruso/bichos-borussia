{
    "plugins": [
        "react"
    ],
    "extends": [
        "eslint:all",
        "plugin:react/all",
        "plugin:react/jsx-runtime"
    ],
    "parserOptions": {
        "ecmaVersion": 2015,
        "sourceType": "module"
    },
    "rules": {
        "sort-imports": "off",
        "one-var": "off",
        "namedComponents": "arrow-function"
    }
}